{
  "name": "template-react",
  "private": true,
  "version": "0.0.3",
  "scripts": {
    "start": "node swEnvbuild.js dev && vite --mode dev",
    "build": "rm -rf dist && node swEnvbuild.js prod && vite build --mode prod",
    "build:dev": "rm -rf dist && node swEnvbuild.js dev && tsc && vite build --mode dev",
    "build:hml": "rm -rf dist && node swEnvbuild.js hml && tsc && vite build --mode hml",
    "serve": "vite preview --port 9001",
    "lint": "tsc && eslint \"src/**/*.{js,jsx,ts,tsx}\" --fix",
    "test": "jest --passWithNoTests",
    "precommit": "yarn lint",
    "pretty": "prettier --config ./.prettierrc.js --write \"src/**/*.{js,jsx,ts,tsx,json}\"",
    "preview": "yarn build && vite preview",
    "deploy": "yarn build && npx firebase deploy --only hosting",
    "generate:component": "npx hygen generator component",
    "generate:scene": "npx hygen generator scene",
    "prepare": "husky install"
  },
  "lint-staged": {
    "**/*.ts?(x)": [
      "yarn pretty",
      "yarn lint"
    ]
  },
  "resolutions": {
    "@types/react": "^18",
    "@types/react-dom": "^18"
  },
  "dependencies": {
    "@fnando/cnpj": "1.0.2",
    "@fnando/cpf": "1.0.2",
    "@platformbuilders/fluid-react": "0.6.1",
    "@platformbuilders/helpers": "^0.7.1",
    "@platformbuilders/krypton-web": "^0.1.1",
    "@platformbuilders/theme-toolkit": "0.1.3",
    "@platformbuilders/validations": "0.0.20",
    "@sentry/react": "7.30.0",
    "@sentry/tracing": "7.30.0",
    "axios": "1.2.2",
    "dotenv": "16.0.3",
    "firebase": "9.15.0",
    "formik": "2.2.9",
    "lodash": "4.17.21",
    "mobx": "6.7.0",
    "mobx-persist": "0.4.1",
    "mobx-persist-store": "^1.1.3",
    "mobx-react": "7.6.0",
    "moment": "^2.29.4",
    "numeral": "2.0.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-gtm-module": "2.0.11",
    "react-helmet": "^6.1.0",
    "react-input-mask": "2.0.4",
    "react-lottie": "1.2.3",
    "react-router-dom": "6.6.2",
    "react-toastify": "9.1.1",
    "styled-components": "5.3.6",
    "web-vitals": "3.1.1",
    "yup": "0.32.11"
  },
  "devDependencies": {
    "@commitlint/cli": "17.4.2",
    "@commitlint/config-conventional": "17.4.2",
    "@platformbuilders/eslint-config-builders": "0.1.2",
    "@types/dompurify": "^2.4.0",
    "@types/history": "^5.0.0",
    "@types/node": "18.11.18",
    "@types/numeral": "2.0.2",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-gtm-module": "2.0.1",
    "@types/react-helmet": "^6.1.6",
    "@types/react-input-mask": "3.0.2",
    "@types/react-lottie": "1.2.6",
    "@types/react-router-dom": "5.3.3",
    "@types/styled-components": "5.1.26",
    "@typescript-eslint/eslint-plugin": "5.48.1",
    "@typescript-eslint/parser": "5.48.1",
    "@vitejs/plugin-react": "1.3.2",
    "eslint": "^8.31.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.6.0",
    "eslint-import-resolver-typescript": "3.5.3",
    "eslint-plugin-import": "2.27.4",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.3",
    "hygen": "6.2.11",
    "jest": "^29.5.0",
    "lint-staged": "13.1.0",
    "prettier": "2.8.2",
    "solidarity": "^3.0.4",
    "typescript": "4.9.4",
    "vite": "2.9.13",
    "vite-plugin-pwa": "0.14.1"
  }
}
